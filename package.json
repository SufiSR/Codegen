{
  "name": "n8n-nodes-plunet",
  "version": "1.0.0",
  "description": "N8N community node for Plunet BusinessManager API integration",
  "keywords": [
    "n8n-community-node-package",
    "plunet",
    "translation",
    "project-management",
    "soap",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/SufiSR/PlunetN8N#readme",
  "author": {
    "name": "SufiSR",
    "email": "sufian@gmx.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SufiSR/PlunetN8N.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",              // ‚Üê if no gulpfile/build:icons, change to just:  "tsc"
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PlunetApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Plunet/Plunet.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.45.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.
